<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper    
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tianjunwei.log.dao.entity.LogInfo">
	<insert id="info">
		insert into tbl_loginfo (logInfo,exceptionType,createTime,exception) values(#{logInfo},#{exceptionType},#{createTime},#{exception})
	</insert>
	<insert id="debug">
		insert into tbl_loginfo (logInfo,exceptionType,createTime,exception) values(#{logInfo},#{exceptionType},#{createTime},#{exception})
	</insert>
	<insert id="warn">
		insert into tbl_loginfo (logInfo,exceptionType,createTime,exception) values(#{logInfo},#{exceptionType},#{createTime},#{exception})
	</insert>
	<insert id="error">
		insert into tbl_loginfo (logInfo,exceptionType,createTime,exception) values(#{logInfo},#{exceptionType},#{createTime},#{exception})
	</insert>
</mapper>